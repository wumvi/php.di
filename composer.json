{
    "name": "wumvi/di",
    "autoload": {
        "psr-4": {
            "Wumvi\\DI\\": "src"
        }
    },
    "require": {
        "symfony/dependency-injection": "v6.0.2",
        "symfony/config": "v6.0.2",
        "symfony/yaml": "v6.0.2",
        "symfony/dotenv": "v6.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.11",
        "squizlabs/php_codesniffer": "3.6.2",
        "overtrue/phplint": "4.1.0",
        "phpstan/phpstan": "1.3.0",
        "phan/phan": "5.3.1"
    },
    "archive": {
        "exclude": ["phpunit", "bootstrap.php", "phpstan.neon", "phpunit.xml", "README.md", "ruleset.xml", ".phpstorm.meta.php", ".travis.yml", ".phplint.yml", ".gitignore"]
    },
    "scripts": {
        "phpstan": [
            "phpstan analyze -c phpstan.neon"
        ],
        "phpunit": [

            "XDEBUG_MODE=coverage phpunit"
        ],
        "phplint": [
            "phplint"
        ],
        "phpcs": [
            "phpcs src --parallel=10 --runtime-set php_version 80800 -p --standard=ruleset.xml"
        ],
        "test": [
            "@phpstan",
            "@phpunit",
            "@phplint",
            "@phpcs"
        ]
    },
    "license": "proprietary"
}
