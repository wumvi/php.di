{
    "name": "wumvi/php.di",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "Wumvi\\DI\\": "src"
        }
    },
    "require": {
        "symfony/dependency-injection": "^v7.3.6",
        "symfony/config": "^v7.3.6",
        "symfony/yaml": "^v7.3.5",
        "symfony/dotenv": "^v7.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "12.4.4",
        "squizlabs/php_codesniffer": "4.0.1",
        "overtrue/phplint": "9.6.2",
        "phpstan/phpstan": "2.1.32",
        "phan/phan": "5.5.2"
    },
    "archive": {
        "exclude": ["phpunit", "bootstrap.php", "phpstan.neon", "phpunit.xml", "README.md", "ruleset.xml", ".phpstorm.meta.php", ".travis.yml", ".phplint.yml", ".gitignore"]
    },
    "scripts": {
        "phpstan": [
            "phpstan analyze -c phpstan.neon"
        ],
        "phpunit": [
            "XDEBUG_MODE=coverage phpunit"
        ],
        "phplint": [
            "phplint --cache /tmp/phplint.cache"
        ],
        "phpcs": [
            "phpcs src --parallel=10 --runtime-set php_version 80415 -p --standard=ruleset.xml"
        ],
        "test": [
            "@phpstan",
            "@phpunit",
            "@phplint",
            "@phpcs"
        ]
    },
    "license": "proprietary"
}
